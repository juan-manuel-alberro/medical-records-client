{
  "name": "medical-records-client",
  "version": "0.0.5",
  "description": "Medical records client",
  "main": "webpack.config.js",
  "scripts": {
    "clear": "rimraf dist",
    "dev": "cross-env NODE_ENV=development webpack --progress --colors --config webpack.config.dev.js",
    "build": "npm run clear && cross-env NODE_ENV=production webpack --progress --colors --config webpack.config.prod.js",
    "start": "node server.js",
    "test": "cross-env NODE_ENV=testing ava --verbose",
    "coverage": "cross-env NODE_ENV=testing nyc ava --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:juan-manuel-alberro/medical-records-client.git"
  },
  "keywords": [],
  "author": "juan-manuel-alberro",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/juan-manuel-alberro/medical-records-client/issues"
  },
  "homepage": "https://github.com/juan-manuel-alberro/medical-records-client/#readme",
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-runtime"
    ],
    "env": {
      "development": {
        "sourceMaps": "inline"
      }
    }
  },
  "nyc": {
    "lines": 99,
    "statements": 99,
    "functions": 99,
    "branches": 99,
    "include": [
      "src/**/*.js"
    ]
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "files": [
      "./tests/**/*.test.js"
    ],
    "concurrency": 5,
    "failFast": true,
    "babel": "inherit"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.1",
    "ava": "^0.16.0",
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.2",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.1.0",
    "babel-register": "^6.14.0",
    "coveralls": "^2.11.12",
    "cross-env": "^2.0.1",
    "css-loader": "^0.24.0",
    "eventsource-polyfill": "^0.9.6",
    "file-loader": "^0.9.0",
    "history": "^3.2.1",
    "html-webpack-plugin": "^2.8.1",
    "nyc": "^8.1.0",
    "postcss-cssnext": "^2.4.0",
    "postcss-import": "^8.0.2",
    "postcss-loader": "^0.11.1",
    "react": "^15.3.1",
    "react-addons-css-transition-group": "^15.3.1",
    "react-addons-test-utils": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-hot-loader": "^3.0.0-beta.3",
    "react-router": "^2.0.0",
    "rimraf": "^2.5.1",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.13",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-dev-server": "^1.15.1",
    "webpack-hot-middleware": "^2.6.4"
  },
  "dependencies": {
    "react-bootstrap-table": "^2.5.1",
    "react-loader": "^2.4.0",
    "superagent": "^2.2.0"
  }
}
